generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
  ID String @id @default(cuid())
  Username String
  Email String @unique
  Password String
}

model Assets{
  ID String @id @default(cuid())
  Company_name  String?
  Product_name String
  Asset_type String
  Url String
}